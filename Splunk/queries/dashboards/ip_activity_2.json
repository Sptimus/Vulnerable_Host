			},
			"name": "Requests overview"
		},
		"ds_XnB871Pd": {
			"type": "ds.search",
			"options": {
				"query": "| inputlookup flagging | search indicator=$ip$ | eval time=strftime(time,\"%d/%m/%y %H:%M:%S\") "
			},
			"name": "flagging"
		}
	},
	"defaults": {
